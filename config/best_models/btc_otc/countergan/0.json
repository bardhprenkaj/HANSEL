                "ce_binarization_threshold": 0.5,
                "device": "cpu",
                "batch_size_ratio": 0.1
            }
        }
    ],
    "evaluation_metrics": [ 
        {"name": "graph_edit_distance", "parameters": {}},
        {"name": "oracle_calls", "parameters": {}},
        {"name": "correctness", "parameters": {}},
