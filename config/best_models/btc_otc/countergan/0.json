{
    "store_paths": [
        {"name": "dataset_store_path", "address": "./data/datasets/"},
        {"name": "embedder_store_path", "address": "./data/embedders/"},
        {"name": "oracle_store_path", "address": "./data/oracles/"},
        {"name": "explainer_store_path", "address": "./data/explainers/BTC_OTC/countergan"},
        {"name": "output_store_path", "address": "./output/BTC_OTC/"}
    ],
    "datasets": [
        {
            "name": "btc_otc", 
            "parameters": {
                "begin_time": 2011,
                "end_time": 2015,
                "filter_min_graphs": 4,
