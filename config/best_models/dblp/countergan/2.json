{
    "store_paths": [
        {"name": "dataset_store_path", "address": "./data/datasets/"},
        {"name": "embedder_store_path", "address": "./data/embedders/"},
        {"name": "oracle_store_path", "address": "./data/oracles/"},
        {"name": "explainer_store_path", "address": "./data/explainers/countergan"},
        {"name": "output_store_path", "address": "./output/tree-cycles-dynamic"}
    ],
    "datasets": [
        {
            "name": "coauthorship_dblp", 
            "parameters": {
                "begin_time": 2000,
                "end_time": 2010,
                "sampling_ratio": 0.2,
                "percentile": 75,
                "min_nodes_per_egonet": 10,
                "features_dim": 10
            } 
        }
    ],
    "oracles": [
        {
            "name": "dynamic_oracle",
            "parameters": {
                "base_oracle" : {
                    "name": "dblp_coauthorship_custom_oracle", 
                    "parameters": {
                        "fold_id": 0,
                        "fit": "true"
                    }
                },
                "first_train_timestamp": 2000
            }
        }
    ],
    "explainers": [
        {
            "name": "countergan",
            "parameters": {
                "n_nodes": 28,
                "fold_id": 2,
                "training_iterations": 250,
                "n_discriminator_steps": 3,
                "n_generator_steps": 2,
                "n_labels": 2,
                "ce_binarization_threshold": 0.5,
                "device": "cpu",
                "batch_size_ratio": 0.1
            }
        }
    ],
    "evaluation_metrics": [ 
        {"name": "graph_edit_distance", "parameters": {}},
        {"name": "oracle_calls", "parameters": {}},
        {"name": "correctness", "parameters": {}},
        {"name": "sparsity", "parameters": {}},
        {"name": "fidelity", "parameters": {}},
        {"name": "oracle_accuracy", "parameters": {}}
    ]
}
